{ 
  // Required
  "manifest_version": 2,
  "name": "MindTab",
  "version": "0.1",

  // Recommended
  "description": "Mindfulness on every new tab",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["file:///mindful/*"],
      "js": ["jquery.js", "gradient.js"]
    }
  ]
}